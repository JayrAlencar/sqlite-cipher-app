{
  "name": "sqlite-cipher-app",
  "version": "0.0.29",
  "description": "App to handler sqlite database ciphered",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "win32": "electron-packager . sqlite-cipher --platform=win32 --arch=ia32",
    "win64": "electron-packager . sqlite-cipher --platform=win32 --arch=x64",
    "linux32": "electron-packager . sqlite-cipher --platform=linux --arch=ia32",
    "linux64": "electron-packager . sqlite-cipher --platform=linux --arch=x64",
    "osx": "electron-packager . sqlite-cipher --platform=darwin --arch=x64",
    "build": "electron-packager . --all"
  },
  "devDependencies": {
    "electron-prebuilt": "^1.2.0",
    "electron-packager": "^6.0.0",
    "basel-cli": "*",
    "jquery": "^2.2.2",
    "angular": "^1.5.1",
    "angular-route": "^1.5.1",
    "angular-utils-pagination": "^0.11.0",
    "sqlite-cipher": "*",
    "ace-builds": "latest",
    "ace-angular": "latest",
    "ngprogress": "latest"
  },
  "titel": "sqlite-cipher"
}
