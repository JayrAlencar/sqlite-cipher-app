{
  "name": "sqlite-cipher-app",
  "version": "0.1.1",
  "description": "App to handler sqlite database ciphered",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "win32": "electron-packager . sqlite-cipher --platform=win32 --arch=ia32 --icon=img/icon.ico",
    "win64": "electron-packager . sqlite-cipher --platform=win32 --arch=x64 --icon=img/icon.ico",
    "linux32": "electron-packager . sqlite-cipher --platform=linux --arch=ia32 --icon=img/icon.ico",
    "linux64": "electron-packager . sqlite-cipher --platform=linux --arch=x64 --icon=img/icon.ico",
    "osx": "electron-packager . sqlite-cipher --platform=darwin --arch=x64 --icon=img/icon.ico",
    "build": "electron-packager . --all --icon=img/icon.ico",
    "postinstall": "install-app-deps",
    "pack": "build / --win --x64",
    "dist": "build"
  },
  "devDependencies": {
    "ace-angular": "latest",
    "ace-builds": "latest",
    "angular": "^1.5.1",
    "angular-route": "^1.5.1",
    "angular-utils-pagination": "^0.11.0",
    "basel-cli": "*",
    "electron-packager": "^6.0.0",
    "electron-prebuilt": "^1.2.0",
    "jquery": "^2.2.2",
    "json2xls": "^0.1.1",
    "ngprogress": "latest",
    "sqlite-cipher": "*"
  },
  "titel": "sqlite-cipher",
  "build": {
    "app-bundle-id": "sqlite-cipher",
    "app-category-type": "database",
    "win": {
      "iconUrl": "img/icon.ico",
      "path":"./"
    }
  }
}
